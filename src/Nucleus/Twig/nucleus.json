{
    "imports": [
        "../View",
        "../AssetManager",
        "../Configuration"
    ],
    "services": {
        "assetBagTwigExtension": {
            "class": "Nucleus\\Twig\\AssetBagTwigExtension"
        },
        "twigEnvironment": {
            "class": "Nucleus\\Twig\\TwigEnvironment",
            "configuration": 
              {
                  "debug":"%debug%",
                  "cache":"%generatedDirectory%/twig"
              }
        },
        "templateRenderer.twig": {
            "class": "Nucleus\\Twig\\TwigTemplateRenderer"
        }
    }
}
